<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.GridPane?>
<GridPane fx:id="gridPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="idLabel" GridPane.columnIndex="0" />
        <Separator orientation="VERTICAL" GridPane.columnIndex="1" />
        <Label fx:id="nameLabel" GridPane.columnIndex="2" />
    </children>
</GridPane>
